<% if post.embed_link.present? %>
  <% if post.embed_link && post.embed_link.length == 11 %>
    <div class="embed_frame"><iframe width="800" height="450" src="//www.youtube.com/embed/<%= post.embed_link %>"frameborder="0" allowfullscreen></iframe></div>

  <% elsif post.embed_link && post.embed_link.include?("tracks") %>

    <div class="embed_frame"><iframe width="800" height="450" scrolling="no" frameborder="no" src="https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/<%= post.embed_link %>&amp;auto_play=false&amp;hide_related=false&amp;show_comments=true&amp;show_user=true&amp;show_reposts=false&amp;visual=true"></iframe></div>

  <% elsif post.embed_link && post.embed_link.include?("playlists") %>

  <iframe width="100%" height="450" scrolling="no" frameborder="no" src="https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/<%= post.embed_link %>&amp;auto_play=false&amp;hide_related=false&amp;show_comments=true&amp;show_user=true&amp;show_reposts=false&amp;visual=true"></iframe>

  <% elsif post.embed_link && post.embed_link.include?("") %>

    <div class="embed_frame"><iframe src="//player.vimeo.com/video/<%= post.embed_link %>?title=0&amp;byline=0&amp;portrait=0&amp;color=fff963" width="800" height="450" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe></div>


  <% end %>

<% end %>
