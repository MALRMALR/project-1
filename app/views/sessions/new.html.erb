<div class="about_content"><h2>Login</h2></div>
<div id="login_page">

  <%= form_tag sessions_path do %>
  <p>
    <%= label_tag :username %>
    <%= text_field_tag :username %>
  </p>
  <p>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </p>
  <p class="button">
    <%= submit_tag 'Log In' %>
  </p>
  <% end %>
</div>
